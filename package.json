  },
  "devDependencies": {
    "@types/mathjs": "^4.4.1",
    "nodemon": "^1.18.4",
    "ts-node": "^7.0.1",
    "tsconfig-paths": "^3.6.0",
    "tslint": "^5.11.0",
    "tslint-config-airbnb": "^5.11.0",
    "typescript": "^3.1.1"
  }
}
